let arregloA: number[]=new array(6);
let arregloB: number[]=new array(6);
let sumaDeArreglos: number[]=new array(6);
let indice: number;
for(indice=0;indice<6;indice++){
    arregloA[indice]= Number(prompt("ingrese el numero de la posicion $(indice): "));
}
for(indice=0;indice<6;indice++){
    arregloB[indice]= Number(prompt("ingrese el numero de la posicion $(indice): "));
}
for(indice=0;indice<6;indice++){
    sumaDeArreglos[indice]= 0;
}
